{{#if placeUrl}}
	<a href='{{placeUrl}}' target='_blank'><h3>{{name}}</h3></a>
{{else}}
	<h3>{{name}}</h3>
{{/if}}
<span class='address'>{{vicinity}}</span>
{{#if closingTime}}
	<div class='closingTime'>Open until {{closingTime}} today</div>
{{/if}}
<img src='{{icon}}' class='icon' width='53' height='53'>